<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.creecmovil">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_creec"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_creecdos"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".UserActivity"></activity>
        <activity android:name=".TransferActivity" />
        <activity android:name=".SocialActivity" />
        <activity android:name=".MenuActivity" />
        <activity android:name=".ActivityContacto" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>